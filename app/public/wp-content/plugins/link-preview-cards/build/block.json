{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "link-preview-cards/card",
  "title": "Link Preview Cards",
  "category": "embed",
  "icon": "format-image",
  "description": "Display a card for any URL with link preview data.",
  "keywords": [
    "link preview",
    "card",
    "embed",
    "block"
  ],
  "version": "0.1.0",
  "textdomain": "link-preview-cards",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "url": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "image": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "align": [
      "left",
      "center",
      "right",
      "wide",
      "full"
    ]
  }
}