      "type": "boolean",
      "default": true
    },
    "autoSpeed": {
      "type": "number",
      "default": 7000
    },
    "transSpeed": {
      "type": "number",
      "default": 400
    },
    "slidesScroll": {
      "type": "string",
      "default": "1"
